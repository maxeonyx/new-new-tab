{
    "manifest_version": 2,
    "name": "New New Tab",
    "version": "1.4.2",
    "description": "Custom New Tab page",
    "icons": {
        "48": "logo.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Google",
            "search_url": "https://www.google.com/search?q={searchTerm}",
            "favicon_url": "https://www.google.com/favicon.ico",
            "keyword": "google"
        },
        "homepage": "index.html"
    }
}
